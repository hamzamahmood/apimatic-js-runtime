{
  "version": "0.5.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "sideEffects": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10.4.0"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig ./tsconfig.build.json",
    "build": "tsdx build --tsconfig ./tsconfig.build.json",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build --tsconfig ./tsconfig.build.json",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@apimatic/schema",
  "author": "Mehdi Jaffery",
  "module": "dist/schema.esm.js",
  "size-limit": [
    {
      "path": "dist/schema.cjs.production.min.js",
      "limit": "5 KB"
    },
    {
      "path": "dist/schema.esm.js",
      "limit": "5 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/lodash.flatten": "^4.4.6",
    "husky": "^4.3.0",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "lodash.flatten": "^4.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:apimatic/apimatic-js-runtime.git"
  }
}
